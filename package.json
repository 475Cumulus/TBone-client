{
  "name": "@475cumulus/tbone",
  "version": "0.1.0",
  "description": "Client library for TBone-based backend",
  "main": "lib/tbone.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "debug": "mocha --inspect-brk --require babel-core/register ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "keywords": [
    "tbone",
    "rest",
    "websockets",
    "realtime",
    "full-duplex"
  ],
  "author": "475 Cumulus Ltd.",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.12.0",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "sinon": "^4.1.2",
    "webpack": "^3.8.1",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "es6-promise": "^4.1.1"
  }
}
